@model IEnumerable<Students_Portal_App.Models.Entities.VwStudentsportal>
<h2>Using the concept of Views in mysql</h2>
<table class="table table-bordered" border="1" >
   <thead>
       <tr>
        <th>Student ID</th>
        <th>Register Number</th>
        <th>Student Status</th>
        <th>Student Name</th>
        <th>Department</th>
        <th>In Time</th>
        <th>Out Time</th>
        <th>Paper Code</th>
        <th>Paper Title</th>
       </tr>
   </thead>       
    <tbody>
        @foreach (var vw in Model)
        {
            <tr>
            <td>@vw.StudentId</td>
            <td>@vw.RegisterNumber</td>
            <td>@vw.StudentStatus</td>
            <td>@vw.StudentName</td>
            <td>@vw.Department</td>
            <td>@vw.InTime</td>
            <td>@vw.OutTime</td>
            <td>@vw.PaperCode</td>
            <td>@vw.PaperTitle</td>
            </tr>
        }
    </tbody>
</table>

